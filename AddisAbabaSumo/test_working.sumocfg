<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <input>
        <net-file value="AddisAbaba.net.xml"/>
        <route-files value="../working_routes.xml"/>
    </input>
    <time>
        <begin value="0"/>
        <end value="3600"/>
        <step-length value="1"/>
